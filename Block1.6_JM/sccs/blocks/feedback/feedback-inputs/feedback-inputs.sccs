.feedback-inputs__container {
  margin-bottom: 16px;
  .feedback-inputs__item {
    height: 24px;
    width: 240px;
    padding: 12px;
    border-radius: 8px;
    border: 1px solid $color-divider;
    font-family: $font-regular;
    @media only screen and (min-width: $screen-desktop) {
      width: 366px;
    }
    &_message {
      height: 118px;
      resize: none;
    }
  }

}
