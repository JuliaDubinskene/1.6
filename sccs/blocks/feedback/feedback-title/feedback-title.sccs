.feedback-menu__title {
  font-family: $font-bold;
  line-height: $line-height-less;
  font-size: 24px;
  position: relative;
  padding-bottom: 16px;
  &::before {
    content:"";
    position: absolute;
    height: 32px;
    width: 2px;
    left: -16px;
    background-color: $color-border;
    border: 1px solid $color-border;
    border-top-right-radius: 22%;
    border-bottom-right-radius: 22%;
  }
}
